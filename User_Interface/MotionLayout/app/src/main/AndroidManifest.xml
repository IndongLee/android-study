<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.indong.motionlayout">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme.NoActionBar">
        <activity android:name=".DrawerLayout2Activity"></activity>
        <activity android:name=".DrawerLayoutActivity" />
        <activity android:name=".CoordinatorLayout2Activity" />
        <activity android:name=".CoordinatorLayoutActivity" />
        <activity android:name=".KeyframeCycleActivity" />
        <activity android:name=".KeyframeInterpolationActivity" />
        <activity android:name=".KeyframeActivity" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".BackgroundColorActivity" />
        <activity android:name=".ImageTransitionActivity" />
    </application>

</manifest>